{
    "components": {
        "navigation": {
            "enabled": true,
            "update_frequency": 100.0,
            "filter_type": "perfect",
            "gps_noise_std": 0.1,
            "imu_noise_std": 0.01,
            "kalman_filter": {
                "process_noise": 0.01,
                "measurement_noise": 0.1,
                "initial_covariance": 1.0,
                "state_dimension": 12,
                "measurement_dimension": 6
            },
            "initialization": {
                "auto_initialize": true,
                "initial_position_uncertainty": 10.0,
                "initial_velocity_uncertainty": 1.0
            }
        },
        "guidance": {
            "enabled": true,
            "update_frequency": 50.0,
            "waypoint_tolerance": 1.0,
            "max_speed": 10.0,
            "path_planning": {
                "algorithm": "a_star",
                "grid_resolution": 0.5,
                "safety_margin": 2.0,
                "replanning_enabled": true,
                "replanning_frequency": 1.0
            },
            "trajectory_generation": {
                "method": "polynomial",
                "smoothing_factor": 0.1,
                "max_acceleration": 5.0,
                "max_jerk": 2.0
            }
        },
        "control": {
            "enabled": true,
            "update_frequency": 200.0,
            "controller_type": "pid",
            "pid_gains": {
                "position": {
                    "kp": 1.0,
                    "ki": 0.1,
                    "kd": 0.01
                },
                "velocity": {
                    "kp": 0.5,
                    "ki": 0.05,
                    "kd": 0.005
                },
                "attitude": {
                    "kp": 2.0,
                    "ki": 0.2,
                    "kd": 0.02
                }
            },
            "control_limits": {
                "max_thrust": 100.0,
                "max_steering_angle": 45.0,
                "max_acceleration": 5.0,
                "max_angular_velocity": 30.0
            },
            "anti_windup": {
                "enabled": true,
                "method": "conditional_integration",
                "saturation_threshold": 0.9
            }
        }
    }
}